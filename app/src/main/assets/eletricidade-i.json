            {
                "name": "Potência Elétrica (Corrente e Tensão)",
                "description": "Potência elétrica é dada pelo produto da tensão e corrente elétrica.",
                "latex": ["P=V\\times I"],
                "constants": {},
                "variables": {
                    "P": "Potência Elétrica (W)",
                    "V": "Diferença de Potencial (V)",
                    "I": "Corrente Elétrica (A)"
                }
            },
            {
                "name": "Quantidade de Calor",
